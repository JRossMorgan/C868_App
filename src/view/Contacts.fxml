<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ContactsController">
   <children>
      <Label alignment="CENTER" layoutX="22.0" layoutY="21.0" prefHeight="40.0" prefWidth="171.0" text="Contact Schedule">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Label>
      <TableView fx:id="contactsTable" layoutX="246.0" layoutY="106.0" prefHeight="200.0" prefWidth="334.0">
        <columns>
          <TableColumn fx:id="contactId" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="contactName" prefWidth="97.0" text="Name" />
            <TableColumn fx:id="contactEmail" prefWidth="161.0" text="Email" />
        </columns>
      </TableView>
      <Button fx:id="home" layoutX="32.0" layoutY="532.0" mnemonicParsing="false" onAction="#onHome" text="Home" />
      <DialogPane fx:id="contactDialog" layoutX="57.0" layoutY="318.0" prefHeight="200.0" prefWidth="616.0" />
   </children>
</AnchorPane>
